{
  "name": "my-qwik-basic-starter",
  "description": "Demo App with Routing built-in (recommended)",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "scripts": {
    "build": "pnpm run build.uno && qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/cloudflare-pages/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "build.uno": "npx unocss \"./src/**\" -o ./src/uno.css",
    "deploy": "wrangler pages publish ./dist",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "serve": "wrangler pages dev ./dist",
    "start": "vite --open --mode ssr",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.1.5",
    "@builder.io/qwik-city": "^1.1.5",
    "@cloudflare/workers-types": "^4.20230518.0",
    "@types/eslint": "8.40.2",
    "@types/node": "^20.3.1",
    "@types/prismjs": "^1.26.0",
    "@typescript-eslint/eslint-plugin": "5.60.0",
    "@typescript-eslint/parser": "5.60.0",
    "@unocss/cli": "0.53.1",
    "@unocss/preset-icons": "^0.53.1",
    "@unocss/preset-web-fonts": "^0.53.1",
    "@unocss/preset-wind": "^0.53.1",
    "@unocss/reset": "^0.53.1",
    "@unocss/transformer-directives": "^0.53.1",
    "@vitest/ui": "^0.32.2",
    "eslint": "8.43.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-qwik": "^1.1.5",
    "jsdom": "~22.1.0",
    "less": "~4.1.3",
    "node-fetch": "~3.3.1",
    "prettier": "2.8.8",
    "typescript": "5.1.3",
    "undici": "5.22.1",
    "unocss": "^0.53.1",
    "unocss-preset-theme": "^0.7.0",
    "vite": "4.3.9",
    "vite-tsconfig-paths": "4.2.0",
    "wrangler": "^2.20.0"
  },
  "dependencies": {
    "@cloudflare/pages-plugin-sentry": "^1.1.1",
    "@julr/unocss-preset-forms": "0.0.5",
    "@qwikbits/headless-ui": "^0.6.7",
    "@qwikbits/utils": "^0.6.7",
    "@sentry/browser": "^7.56.0",
    "cal-sans": "^1.0.1",
    "class-variance-authority": "^0.6.0",
    "prismjs": "1.29.0",
    "toucan-js": "^3.1.0"
  }
}
